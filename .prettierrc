{
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "endOfLine": "lf",
  "overrides": [
    {
      "files": ["servers/rspc/src/**/*.rs"],
      "options": {
        "tabWidth": 4,
        "printWidth": 100
      }
    }
  ],
  "plugins": ["./node_modules/prettier-plugin-rust"]
}
